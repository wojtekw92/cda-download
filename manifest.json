{
  "manifest_version": 2,

  "name": "mp4 download",
  "description": "This extension find mp4 files and show them to download",
  "version": "1.0",

  "browser_action": {
    "default_icon": "download.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
